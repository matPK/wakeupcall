{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Wakeupcall: Bot",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/src/bot/index.js",
      "cwd": "${workspaceFolder}",
      "envFile": "${workspaceFolder}/.env",
      "console": "integratedTerminal",
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "name": "Wakeupcall: Runner (Once)",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/src/runner/index.js",
      "cwd": "${workspaceFolder}",
      "envFile": "${workspaceFolder}/.env",
      "console": "integratedTerminal",
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "name": "Wakeupcall: Migrate",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/src/db/migrate.js",
      "cwd": "${workspaceFolder}",
      "envFile": "${workspaceFolder}/.env",
      "console": "integratedTerminal",
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "name": "Wakeupcall: Rollback (One)",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/src/db/migrateDown.js",
      "cwd": "${workspaceFolder}",
      "envFile": "${workspaceFolder}/.env",
      "console": "integratedTerminal",
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "name": "Wakeupcall: Attach (Port 9229)",
      "type": "node",
      "request": "attach",
      "port": 9229,
      "restart": true,
      "skipFiles": [
        "<node_internals>/**"
      ]
    }
  ]
}
